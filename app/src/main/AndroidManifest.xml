<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/metaverse"
        android:label="@string/app_name"
        android:roundIcon="@drawable/metaverse"
        android:supportsRtl="true"
        android:theme="@style/Theme.BottomAndNav"
        tools:targetApi="31">
        <activity
            android:name=".activities.ChildContainerActivity"
            android:exported="false" />
        <activity
            android:name=".fragments.CreditLimit.CreditLimitView"
            android:exported="false" />
        <activity
            android:name=".fragments.CreditLimit.CreditLimitEnhancement"
            android:exported="false" />
        <activity
            android:name=".activities.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:exported="true"></activity>
    </application>

</manifest>